  <Prerequisites/>
  <Ingredients>
    <Ingredient>
      <Name>Ground Beef</Name>
      <Quantity units="POUND">1</Quantity>
      <Prep>NONE</Prep>
      <Notes/>
    </Ingredient>
    <Ingredient>
      <Name>Yellow Onion</Name>
      <Quantity units="CUP">.5</Quantity>
      <Prep>MINCED</Prep>
      <Notes/>
    </Ingredient>
    <Ingredient>
      <Name>Juice from canned black olives</Name>
      <Quantity units="CUP">.5</Quantity>
      <Prep>NONE</Prep>
      <Notes/>
