<?xml version="1.0"?>
<Recipe>
  <Title image="Catfish Nuggets_CatfishNuggets.jpg" name="Catfish Nuggets" category="ENTREE_FISH" cuisine="AMERICAN">
    <Yield amount="1.5" units="POUND"/>
    <Times prep="20" cook="10"/>
  </Title>
  <Nutrition calories="" fat="" carbs="" protein="" sugar="" sodium="" fiber="" servings="" servingsUnit="CUP"/>
  <Description>Catfish nuggets in a crispy batter.</Description>
  <Prerequisites>
    <Prerequisite>Preheat oil in deep fryer to 350F.</Prerequisite>
  </Prerequisites>
  <Ingredients>
    <Ingredient>
      <Name>Catfish</Name>
      <Quantity units="POUND">1.5</Quantity>
      <Prep>CHOPCOARSE</Prep>
      <Notes>Bite sized nuggets</Notes>
    </Ingredient>
    <Ingredient>
      <Name>AP flour</Name>
      <Quantity units="CUP">1</Quantity>
      <Prep>NONE</Prep>
      <Notes>For dredging</Notes>
    </Ingredient>
    <Ingredient>
      <Name>Eggs</Name>
      <Quantity units="EACH">2</Quantity>
      <Prep>NONE</Prep>
      <Notes>Whisked</Notes>
    </Ingredient>
    <Ingredient>
      <Name>Hot sauce</Name>
      <Quantity units="TBSP">3</Quantity>
      <Prep>NONE</Prep>
      <Notes>Frank's</Notes>
    </Ingredient>
    <Ingredient>
      <Name>Salt</Name>
      <Quantity units="TBSP">1</Quantity>
      <Prep>NONE</Prep>
      <Notes/>
    </Ingredient>
    <Ingredient>
      <Name>Pepper</Name>
      <Quantity units="TSP">.5</Quantity>
      <Prep>GROUND</Prep>
      <Notes/>
    </Ingredient>
    <Ingredient>
      <Name>Corn meal fish breading</Name>
      <Quantity units="PACKAGE">1</Quantity>
      <Prep>GROUND</Prep>
      <Notes>Andy's</Notes>
    </Ingredient>
  </Ingredients>
  <Method>
    <Step image="">Whisk eggs and hot sauce together.</Step>
    <Step image="">Season catfish nuggets with salt and pepper.</Step>
    <Step image="Catfish%20Nuggets_20250430_160050.jpg">Dredge catfish nuggets in flour.</Step>
    <Step image="Catfish%20Nuggets_20250430_160054.jpg">Coat catfish nuggets with egg mixture and then in corn meal breading.</Step>
    <Step image="Catfish%20Nuggets_20250430_160859.jpg">Let nuggets stand while fryer is preheating.</Step>
    <Step image="">Fry nuggets until GBD.</Step>
  </Method>
  <Variations>
    <Variation>Serve with &lt;a href='/GetRecipe?recipeName=Tartar%20sauce'&gt;tartar sauce&lt;/a&gt;.</Variation>
  </Variations>
</Recipe>